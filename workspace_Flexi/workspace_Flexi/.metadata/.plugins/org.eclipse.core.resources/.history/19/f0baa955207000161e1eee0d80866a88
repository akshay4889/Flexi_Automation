package com.fl.test;

import org.testng.annotations.Test;
import org.testng.annotations.BeforeTest;
import org.testng.annotations.AfterTest;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.firefox.FirefoxDriver;
import com.fl.test.GenericKeywords;


public class AutomationTestCases {
	
	WebDriver driver;
	GenericKeywords keywords = new GenericKeywords();
	
	@Test
	public void loginToApp() {
		keywords.login(keywords.prop.getProperty("EmailID")+"@"+keywords.prop.getProperty("EmailIDend"), keywords.prop.getProperty("Password"));
	}
	
	@BeforeTest
	public void beforeTest() {
		driver = new FirefoxDriver();
		driver.manage().window().maximize();
	}

	@AfterTest
	public void afterTest() {
		driver.quit();
	}

}
